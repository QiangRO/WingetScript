{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}'s </>",
          "foreground": "#E06C75",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uF0E7 root <#ffffff>in</> ",
          "foreground": "#E06C75",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_short",
            "max_depth": 2,
            "hide_root_location": true
          },
          "template": "{{ .Path }} ",
          "foreground": "#55B9C4",
          "type": "path",
          "style": "plain"
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#C678DD",
          "properties": {
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }} {{ .BranchStatus }}<#98C379>{{ if or (.Staging.Added) (.Staging.Modified) (.Staging.Deleted) }} \uF046 \u002B{{ .Staging.Added }} \u007E{{ .Staging.Modified }} \u2212{{ .Staging.Deleted }}{{ end }}</><#E06C75>{{ if or (.Working.Untracked) (.Working.Modified) (.Working.Deleted) }} \uF044 \u002B{{ .Working.Untracked }} \u007E{{ .Working.Modified }} \u2212{{ .Working.Deleted }}{{ end }}{{ if .Merge }} !{{ end }}{{ if gt .StashCount 0 }} \uF0C7 {{ .StashCount }}{{ end }}"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#4063D8>angular </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#FF6062",
          "type": "angular",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": true
          },
          "template": "<#ffffff>via </><#4063D8>crystal </>{{ .Profile }}{{ if .Region }}<#ffffff>@</>{{ .Region }}{{ end }}",
          "foreground": "#FF9C08",
          "type": "aws",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#4063D8>crystal </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#59C7F8",
          "type": "flutter",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_package_manager": true,
            "fetch_version": true,
            "npm_icon": "<#C94A16>\ue71e npm</>",
            "yarn_icon": "<#44FFD2>\uf487 yarn</>"
          },
          "template": "<#ffffff>via </>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}\ue718 {{ .Full }} ",
          "foreground": "#98C379",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#4063D8>crystal </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#4063D8",
          "type": "crystal",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#DE3F24>ruby </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#DE3F24",
          "type": "ruby",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#DE3F24>rust </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#DE3F24",
          "type": "rust",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": "<#ffffff>via </><#306998>python </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#306998",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#906cff>kotlin </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#906cff",
          "type": "kotlin",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#ec2729>java </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ec2729",
          "type": "java",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#787CB5>php </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#787CB5",
          "type": "php",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": "<#ffffff>via </><#F0563E>switf </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#F0563E",
          "type": "swift",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .Error }}<#e91e63>X</>{{ end }} ",
          "foreground": "#C94A16",
          "type": "status",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "template": " {{ .FormattedMs }} \uf252 ",
          "foreground": "#ffffff",
          "type": "executiontime",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "âžœ ",
          "foreground": "#63F08C",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}